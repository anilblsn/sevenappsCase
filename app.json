{
  "expo": {
    "name": "sevenappsCase",
    "slug": "sevenappsCase",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sevenappscase",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to select videos.",
          "cameraPermission": "The app needs access to your camera to record videos."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to select videos.",
        "NSPhotoLibraryAddUsageDescription": "This app needs access to save videos to your photo library."
      },
      "bundleIdentifier": "com.anilss.sevenappsCase",
      "appleTeamId": "29V9MQZP46"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.anilss.sevenappsCase"
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
